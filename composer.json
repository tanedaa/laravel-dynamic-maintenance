{
    "name": "tanedaa/laravel-dynamic-maintenance",
    "description": "A Laravel package to enable maintenance mode for specific named routes",
    "type": "library",
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "illuminate/support": "^10.0|^11.0"
    },
    "autoload": {
        "psr-4": {
            "Tanedaa\\LaravelDynamicMaintenance\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Tanedaa\\LaravelDynamicMaintenance\\ServiceProvider"
            ]
        }
    },
    "authors": [
        {
            "name": "Taneda",
            "email": "info@taneda.dev"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "version": "1.1.0"
}